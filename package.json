{
    "name": "express-HelloWorld",
    "description": "Hello World Node Express web application",
    "version": "0.0.1",
    "type": "module",
    "private": true,
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "4.18.2",
        "express-async-errors": "^3.1.1",
        "mongodb": "^5.7.0"
    },
    "devDependencies": {
        "mocha": "^10.2.0",
        "supertest": "^6.3.3",
        "unit.js": "^2.1.1",
        "concurrently": "=7.0.0"
    },
    "scripts": {
        "start": "concurrently 'node server.js' 'cd frontend/src && npm start'",
